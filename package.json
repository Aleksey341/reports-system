{
  "name": "reports-system",
  "version": "1.1.2",
  "description": "Sistema de reportes municipales con dashboard y exportación",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "build": "echo 'No build step required'",
    "db:init": "node scripts/init-database.js",
    "db:check": "node scripts/check-db.js",
    "db:migrate": "node migrate.js --file \"./муниципалитеты с главами и должностями.xlsx\" --sheet \"Лист1\"",
    "db:clear": "node scripts/clear-test-data.js",
    "seed": "node seed.js",
    "create-admin": "node scripts/create-admin.js",
    "postinstall": "echo 'Run: npm run db:init to initialize database'"
  },
  "dependencies": {
    "bcrypt": "^6.0.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "exceljs": "^4.4.0",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-session": "^1.18.2",
    "helmet": "^7.1.0",
    "moment": "^2.29.4",
    "multer": "^2.0.2",
    "pg": "^8.11.3",
    "uuid": "^9.0.1",
    "xlsx": "^0.18.5",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "reports",
    "dashboard",
    "postgresql",
    "amvera"
  ],
  "author": "alex1976",
  "license": "MIT"
}
